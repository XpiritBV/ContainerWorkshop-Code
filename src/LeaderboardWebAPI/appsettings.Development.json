{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "Ready",
        "Uri": "http://localhost:8080/health/ready"
      },
      {
        "Name": "Lively",
        "Uri": "http://localhost:8080/health/lively"
      }
    ],
    "Webhooks": [],
    "EvaluationTimeOnSeconds": 5,
    "MinimumSecondsBetweenFailureNotifications": 30
  }
}
